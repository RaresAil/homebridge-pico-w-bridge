    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "homebridge": "^1.5.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "minimist": "^1.2.6"
